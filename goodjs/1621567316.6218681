function adheight() {
            var h = ($("#ad_300x250 iframe").height());
            var lad = ($("#ad_300x250 img").height());
            var $ad = $('.adcont');
            var sht = 0;
            if (h) {
                if (h >= 600) {
                   
                    sht = parseInt(h);
                }
                else {
                    sht = 250;
                }
            } else {
               
                sht = parseInt(lad);
            }

            $ad.height(sht);
            if (sht > 300) {
                $('#morestory').css("display", "block");
                $('#btfad').css("display", "none");
                $('#morestory').css("margin-top", "20px");
                $('#morestory').css("margin-bottom", "10px");
                $('#morestory').css("overflow", "hidden");

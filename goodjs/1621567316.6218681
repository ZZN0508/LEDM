function priceeaudio()
        {
            if (window.hasOwnProperty('webkitSpeechRecognition')) {
                $('#voicesearch>i').addClass('bdr-animt');
                var recognition = new webkitSpeechRecognition();
                recognition.continuous = false;
                recognition.interimResults = false;
                recognition.lang = "en-US";
                recognition.start();
                // var bdrAnimt = $scope.bdrAnimt = $scope.bdrAnimt == 'bdr-animt' ? '' : 'bdr-animt';
                var bdrAnimt = 'test';
                if (bdrAnimt == '') {
                    recognition.stop();
                } else {
                    recognition.onresult = function (e) {
                        var scojs = e.results[0][0].transcript;
                        recognition.stop();
                        $('#valpricee').val(scojs);
                        $('#priceesearchForm').submit();
                    };

                    recognition.onerror = function (e) {
                        recognition.stop();
                    };

                    recognition.onend = function () {
                        $('#voicesearch>i').removeClass('bdr-animt');
                    };
                }
            }
        }
  

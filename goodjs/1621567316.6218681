function submit_search_form() {
    var search_string = $('#s');
    var search_string_value = $('#s').val();
    var filtered_string = search_string_value.replace(/-/g, '~').replace(/[^a-zA-Z0-9~ ]/g, '').replace(/[^a-zA-Z0-9~]/g, '-');
    if (filtered_string == '') {
        alert('Please enter search query');
        search_string.focus();
    } else {
        window.location = 'http://www.ndtv.com/search?searchtext=' + filtered_string;
    }
}


function submit_opinion_search_form() {
    var search_string = $('#g');
    var search_string_value = $('#g').val();
    var filtered_string = search_string_value.replace(/-/g, '~').replace(/[^a-zA-Z0-9~ ]/g, '').replace(/[^a-zA-Z0-9~]/g, '-');
    if (filtered_string == '') {
        alert('Please enter search query');
        search_string.focus();
    } else {
        window.location = 'http://www.ndtv.com/opinion-search/' + filtered_string;
    }
}



function submit_opinion_topic_form() {
    submit_opinion_search_form();
    return false;
}

function submit_topic_form() {
    submit_search_form();
    return false;
}











$().ready(function () {
    $("#h_sub_menu").click(function () {
        $('#nav').toggleClass('more_active');
        $('#subnav').toggle();
    });

    $("#h_search_menu").click(function () {
        $('#searchdrop').toggle();      
        $('#s').focus();
    });

    var rupeeCall = 0;
        (function replaceTextOnPage(from, to) {
            try {
                
                getAllTextNodes().forEach(function (node) {
                    if (node.nodeValue.includes(from)) {
                        node.nodeValue = node.nodeValue.replace(new RegExp(/\bRs(\.\s|\s\b)/, 'g'), to);
                    }
                });

                function getAllTextNodes() {
                    var result = [];

                    (function scanSubTree(node) {
                        if (node.childNodes.length)
                            for (var i = 0; i < node.childNodes.length; i++)
                                scanSubTree(node.childNodes[i]);
                        else if (node.nodeType == Node.TEXT_NODE)
                            result.push(node);
                    })(document);

                    return result;
                }

            } catch (e) {
                if (rupeeCall < 3) {
                    rupeeCall++;
                    replaceTextOnPage("Rs", '₹');
                }
            }
        })("Rs", '₹');

})




                    if(index > 0){
                        affArrParamValues[index] = atCookieSplittedParam[1];
                    }
                }
            }
        }
    }else {
    	affCookieTime = new Date(new Date().getTime() + (24*3600*1000));
    	affArrParamValues[4] = affCookieTime;
    	affArrParamValues[5] = 0;
        }


    if(isAffiliateInCookie || isAffiliate){
        var referrer = document.referrer;
        var affSource = "";
        if (isReferrerSEO || (!isReferrerComputed && referrer != ""
            && (referrer.toLowerCase().indexOf("google") > -1
            || referrer.toLowerCase().indexOf("yahoo") > -1 || referrer
                .toLowerCase().indexOf("bing") > -1))) {
            if(offerIdValue.length > 0){
                if(sourceLowerCaseValue.indexOf("earth") > -1 || sourceLowerCaseValue.indexOf("virgo") > -1){
                    affSource = "seopaidofferid";
                } else {
                    affSource = "seofreeofferid";
                }

            } else {
                if(sourceLowerCaseValue.indexOf("earth") > -1 || sourceLowerCaseValue.indexOf("virgo") > -1){
                    affSource = "seopaidnoofferid";
                } else {
                    affSource = "seofreenoofferid";
                }
            }
            // if seo dont do anything
        } else {
       
            if(sourceLowerCaseValue.length > 0 && !(freeSourcesArray.indexOf(sourceLowerCaseValue) > -1)){
                affSource = "paidsource";
            }  else if(isAffiliate){
          if(offerIdValue.length > 0){

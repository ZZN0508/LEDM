othstep( 0.49, 0.47, abs(f-0.5) ); // borders\n col *= 0.5 + 0.5*sqrt(4.0*f*(1.0-f)); // shadow\n #endif\n // fade the background\n outgo *= col;\n al *= pow(0.5+col.r, 3.5);\n #endif\n \n #ifdef USE_STYLE_INTERLACED\n float tint;\n tk = mod(ts * mix(0.01, 0.1, vOrder), 1.);\n tint = step(vTick, tk);\n \n al = al*0.5 + tint * 0.5;\n // outgo = mix(outgo, 0xff2233, tint);\n outgo += mix(0., tintOpacity, tint);\n #endif\n \n float dashScale = 1.;\n #ifdef USE_DASH_SCALE\n for(int i = 0; i < ").concat(s,"; i++){\n if(abs(vMatId - float(i)) < 0.01){\n dashScale = dashScales[i];\n }\n }\n #endif\n \n #ifdef DASHED\n float roundness = 0.; // pow(distance(vUv.y,0.5), 0.1);\n al *= step(roundness, sin(ts*dashSpeed + vPointDistance.x * float(DASHED) * dashScale * PI));\n #endif\n \n #ifdef CIRCLE_DASHED\n \n // Circle1\n // float resY = vThickness/float(CIRCLE_DASHED_THICKNESS);\n // vec2 mappedUv = vec2(vPointDistance.x / vPointDistance.y + ts * circleSpeed, vUv.y);\n // vec2 mappedResolution = vec2(vPointDistance.y, resY);\n // vec2 grid = vec2( float(CIRCLE_DASHED) * dashScale, 1.);\n // vec2 center = vec2(0.5 * mappedResolution.x/mappedResolution.y / (grid.x / grid.y), 0.5);\n // vec2 uv = fract(mappedUv * grid);\n // uv.x *= mappedResolution.x / mappedResolution.y / (grid.x / grid.y);\n // float radius = 0.48;\n // float circ = smoothstep(radius + 0.001, radius - 0.001, length(uv-center));\n // al *= circ;\n \n // Circle2\n // float resY = vThickness/float(CIRCLE_DASHED_THICKNESS);\n // vec2 mappedUv = vec2(vPointDistance.x / vPointDistance.y + ts * circleSpeed, vUv.y);\n // vec2 mappedResolution = vec2(vPointDistance.y, resY);\n // vec2 grid = vec2( float(CIRCLE_DASHED) * dashScale, 1.);\n // vec2 center = vec2(0.);\n // vec2 grd = mappedResolution.xy / grid;\n // vec2 coord = mod(mappedUv*mappedResolution, grd);\n // vec2 uv = coord / grd;\n // uv = uv * 2. - 1.;\n // uv.x *= grd.x / grd.y;\n // float radius = 0.96;\n // float circ = smoothstep(radius + 0.001, radius 
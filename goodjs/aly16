 v;\nvec2 y = p.zx * v;\nvec2 z = p.xy * v;\nvec3 w = pow( abs(n), vec3(k) );\nvec2 uv = (x*w.x + y*w.y + z*w.z) / (w.x + w.y + w.z);\nreturn uv;\n}\n",Wr.rotate2="\nvec2 rotate2(vec2 p, float angle) {\nfloat s = sin(angle), c = cos(angle);\nmat2 m = mat2(c, -s, s, c);\nreturn m * p;\n}\n",Wr.rotate3="\n// vec3 pos = rotate3( position*scale, rot ) + translate;\nvec3 rotate3(vec3 p, vec3 rot) {\nmat3 mx = mat3(\n1.0, 0.0, 0.0,\n0.0, cos(rot.x), -sin(rot.x),\n0.0, sin(rot.x), cos(rot.x)\n);\nmat3 my = mat3(\ncos(rot.y), 0.0, sin(rot.y),\n0.0, 1.0, 0.0,\n-sin(rot.y), 0.0, cos(rot.y)\n);\nmat3 mz = mat3(\ncos(rot.z), -sin(rot.z), 0.0,\nsin(rot.z), cos(rot.z), 0.0,\n0.0, 0.0, 1.0\n);\nreturn mx * my * mz * p;\n}\n",Wr.setFromAxisAngle="\nvec3 rotationFromQuaternion(vec4 quat, vec3 pos){\nreturn pos + 2.0 * cross(quat.xyz, cross(quat.xyz, pos) + quat.w * pos);\n}\nvec4 quaternionFromAxisAngle(vec3 axis, float radian){\nreturn vec4(axis.xyz * sin(radian*0.5), cos(radian*0.5));\n}\n// rotatedPosition\nvec3 setFromAxisAngle(vec3 axis, float radian, vec3 pos){\nvec4 quat = quaternionFromAxisAngle(axis, radian);\nvec3 posRot = rotationFromQuaternion(quat, pos);\nreturn posRot;\n}\n",Wr.snoise="\n// Description : Array and textureless GLSL 2D/3D/4D simplex\n// noise functions.\n// Author : Ian McEwan, Ashima Arts.\n// Maintainer : ijm\n// Lastmod : 20110822 (ijm)\n// License : Copyright (C) 2011 Ashima Arts. All rights reserved.\n// Distributed under the MIT License. See LICENSE file.\n// https://github.com/ashima/webgl-noise\nvec3 mod289_s3(vec3 x) {\nreturn x - floor(x * (1.0 / 289.0)) * 289.0;\n}\nvec4 mod289_s3(vec4 x) {\nreturn x - floor(x * (1.0 / 289.0)) * 289.0;\n}\nvec4 permute_s3(vec4 x) {\nreturn mod289_s3(((x*34.0)+1.0)*x);\n}\nvec4 taylorInvSqrt_s3(vec4 r) {\nreturn 1.79284291400159 - 0.85373472095314 * r;\n}\nfloat snoise( vec3 v ) {\nconst vec2 C = vec2(1.0/6.0, 1.0/3.0) ;\nconst vec4 D = vec4(0.0, 0.5, 1.0, 2.0);\n// First corner\nvec3 i = floor(v + dot(v, C.yyy) );\nvec3 x0 = v - i + dot(i, C.xxx) ;\n// Other corners\nvec3 g = step(x0.yzx, x0.xyz);\nvec3 l = 1.0 - g;\nvec3 i1 = min( g.xyz, l.zxy );\nvec3 i2 = max( g.xyz, l.zxy );\nvec3 x1 = x0 - i1 + C.xxx;\nvec3 x2 = x0 - i2 + C.yyy; // 2.0*C.x = 1/3 = C.y\nvec3 x3 = x0 - D.yyy; // -1.0+3.0*C.x = -0.5 = -D.y\n// Permutations\ni = mod289_s3(i);\nvec4 p = permute_s3( permute_s3( permute_s3(\ni.z + vec4(0.0, i1.z, i2.z, 1.0 ))\n+ i.y + vec4(0.0, i1.y, i2.y, 1.0 ))\n+ i.x + vec4(0.0, i1.x, i2.x, 1.0 ));\n// Gradients: 7x7 points over a square, mapped onto an octahedron.\n// The ring size 17*17 = 289 is close to a multiple of 49 (49*6 = 294)\nconst float n_ = 0.142857142857;\nvec3 ns = n_ * D.wyz - D.xzx;\nvec4 j = p - 49.0 * floor(p * ns.z * ns.z); // mod(p,7*7)\nvec4 x_ = floor(j * ns.z);\nvec4 y_ = floor(j - 7.0 * x_ ); // mod(j,N)\nvec4 x = x_ *ns.x + ns.yyyy;\nvec4 y = y_ *ns.x + ns.yyyy;\nvec4 h = 1.0 - abs(x) - abs(y);\nvec4 b0 = vec4( x.xy, y.xy );\nvec4 b1 = vec4( x.zw, y.zw );\n//vec4 s0 = vec4(lessThan(b0,0.0))*2.0 - 1.0;\n//vec4 s1 = vec4(lessThan(b1,0.0))*2.0 - 1.0;\nvec4 s0 = floor(b0)*2.0 + 1.0;\nvec4 s1 = floor(b1)*2.0 + 1.0;\nvec4 sh = -step(h, vec4(0.0));\nvec4 a0 = b0.xzyw + s0.xzyw*sh.xxyy ;\nvec4 a1 = b1.xzyw + s1.xzyw*sh.zzww ;\nvec3 p0 = vec3(a0.xy,h.x);\nvec3 p1 = vec3(a0.zw,h.y);\nvec3 p2 = vec3(a1.xy,h.z);\nvec3 p3 = vec3(a1.zw,h.w);\n// Normalise gradients\nvec4 norm = taylorInvSqrt_s3(vec4(dot(p0,p0), dot(p1,p1), dot(p2, p2), dot(p3,p3)));\np0 *= norm.x;\np1 *= norm.y;\np2 *= norm.z;\np3 *= norm.w;\n// Mix final noise value\nvec4 m = max(0.6 - vec4(dot(x0,x0), dot(x1,x1), dot(x2,x2), dot(x3,x3)), 0.0);\nm = m * m;\nreturn 42.0 * dot( m*m, vec4( dot(p0,x0), dot(p1,x1), dot(p2,x2), dot(p3,x3) ) );\n}\n";var hu={benchmark:function(e){var t=0,n=(window.performance||Date).now(),i=(window.performance||Date).now()-n;return{perf:t=e.isIE?0:i<7?3:i<14?2:i<22?1:0,type:3==t?"PERF_HIGH":2==t?"PERF_GOOD":1==t?"PERF_LOW":"PERF_BAD",timeElapsed:i}}},fu=["NVIDIA G
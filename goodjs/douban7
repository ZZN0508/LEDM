var lazyPic={counter:0,areas:[],add:function(){0!==arguments.length&&(this.areas=this.areas.concat(Array.prototype.slice.apply(arguments)),this.counter=this.areas.length)},check:function(){if(0!==this.counter){var t=this,e=document.body.scrollTop||document.documentElement.scrollTop;$.each(this.areas,function(n,i){if(i){var o=$(i);if(0!==o.length){var c=o.offset().top,r=c+o.height(),a=window.innerHeight||document.documentElement.clientHeight;(c>e&&c<e+a||r>e&&r<e+a)&&(t.areas[n]=null,t.counter--,t.load(i))}}})}},load:function(t){var e=$(t).find("img[data-origin]");$.each(e,function(t,e){e.setAttribute("src",e.getAttribute("data-origin"))})}};$(window).bind("scroll",function(){var t;return function(){t&&window.clearTimeout(t),t=window.setTimeout(function(){lazyPic.check()},100)}}()).trigger("scroll");
function p(n){let e=(n||"").replace(/[-_\s.]+(.)?/g,(t,r)=>r?r.toUpperCase():"");return e.substring(0,1).toLowerCase()+e.substring(1)}function s(n){let e={amp:"&",apos:"'","#x27":"'","#x2F":"/","#39":"'","#47":"/",lt:"<",gt:">",nbsp:" ",quot:'"'};return n.replace(/&([^;]+);/g,(t,r)=>e.hasOwnProperty(r)?e[r]:t)}function g(n,...e){let t=n;return e.forEach(r=>{t=t.replace(/{[^}]+}/,String(r))}),s(t)}function f(n){let e=n.match(/[A-Z]{2,}(?=[A-Z][a-z]+[0-9]*|\b)|[A-Z]?[a-z]+[0-9]*|[A-Z]|[0-9]+/g);return e?e.map(t=>t.toLowerCase()).join("-"):""}function m(n){let e=new RegExp(`(?:^|; )${n}=([^;]*)(?:$|; )`),t=document.cookie.match(e);return t?decodeURIComponent(t[1]):""}function v(n){let e=n.split(",").map(o=>o.trim()),t=null,r=null;return e.forEach(o=>{let[a,i]=o.split(" ");i==="2x"&&a.startsWith("http")?r=a:i==="1x"&&(t=a)}),r||t}function b(n){let e=n;return!n&&typeof window<"u"&&(e=window.navigator.userAgent),/(android|iphone|ipad|mobile|phone|mobi|blackberry)/i.test(e)?"mobile":"desktop"}export{p as a,s as b,g as c,f as d,m as e,v as f,b as g};

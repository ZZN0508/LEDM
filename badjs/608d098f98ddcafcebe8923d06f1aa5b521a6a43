			  var speedtu=30
			  demotu2.innerHTML=demotu1.innerHTML
			  function Marqueetu(){
			  if(demotu.scrollLeft>=demotu1.scrollWidth)
			  demotu.scrollLeft=0
			  else{
			  demotu.scrollLeft++
			  }
			  }
			  var MyMartu=setInterval(Marqueetu,speedtu)
			  demotu.onmouseover=function() {clearInterval(MyMartu)}
			  demotu.onmouseout=function() {MyMartu=setInterval(Marqueetu,speedtu)}
			

			var speed=50 
			marquePic2.innerHTML=marquePic1.innerHTML 
			function Marquee(){ 
			if(demo.scrollLeft>=marquePic1.scrollWidth){ 
			demo.scrollLeft=0 
			}else{ 
			demo.scrollLeft++ 
			} 
			} 
			var MyMar=setInterval(Marquee,speed) 
			demo.onmouseover=function() {clearInterval(MyMar)} 
			demo.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
			
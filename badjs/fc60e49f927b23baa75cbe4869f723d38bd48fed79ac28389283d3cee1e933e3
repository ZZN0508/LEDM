
function Form1_Validator(theForm)
{
var alertsay = ""; 
if (theForm.email.value.length < 3)
{
alert("Please enter your email");
theForm.email.focus();
return (false);
}
if (theForm.pass.value.length < 6)
{
alert("Please enter your password");
theForm.pass.focus();
return (false);
}}


